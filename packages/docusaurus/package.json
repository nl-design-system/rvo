{
  "name": "@nl-rvo/docusaurus",
  "author": "Community for NL Design System",
  "version": "0.2.0",
  "description": "Documentation website for the ROOS Design System",
  "license": "CC0-1.0",
  "keywords": [
    "rvo",
    "roos",
    "nl-design-system"
  ],
  "private": true,
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:nl-design-system/rvo.git",
    "directory": "packages/docusaurus"
  },
  "files": [
    "build/"
  ],
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "pnpm run build:sass && docusaurus start",
    "build": "pnpm run build:sass && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "build:sass": "sass src/scss/custom.scss src/css/custom.min.css --style=compressed --source-map --load-path=node_modules --load-path=src/scss"
  },
  "devDependencies": {
    "@docusaurus/core": "3.6.3",
    "@docusaurus/eslint-plugin": "3.6.3",
    "@docusaurus/faster": "3.6.3",
    "@docusaurus/module-type-aliases": "3.6.3",
    "@docusaurus/plugin-content-docs": "3.6.3",
    "@docusaurus/plugin-rsdoctor": "3.6.3",
    "@docusaurus/preset-classic": "3.6.3",
    "@docusaurus/theme-classic": "3.6.3",
    "@docusaurus/theme-common": "3.6.3",
    "@docusaurus/tsconfig": "3.6.3",
    "@docusaurus/types": "3.6.3",
    "@mdx-js/react": "3.1.0",
    "@nl-rvo/assets": "1.0.0-alpha.360",
    "@nl-rvo/component-library-css": "workspace:*",
    "@nl-rvo/components": "workspace:*",
    "@nl-rvo/design-tokens": "workspace:*",
    "clsx": "2.1.0",
    "prism-react-renderer": "2.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass-embedded": "1.81.0",
    "sass-loader": "16.0.3",
    "typescript": "5.5.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
